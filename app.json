{
  "expo": {
    "name": "PaisaPal",
    "slug": "PaisaPal",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      }
    },
    "web": {},
    "assetBundlePatterns": [
      "**/*"
    ],
    "fonts": {
      "Poppins-SemiBold": "./assets/fonts/Poppins-SemiBold.ttf"
    },
    "plugins": [
      "expo-font"
    ]
  }
}
